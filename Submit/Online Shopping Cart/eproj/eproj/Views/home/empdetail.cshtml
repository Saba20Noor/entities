@model IEnumerable<eproj.Models.employee>

@{
    ViewBag.Title = "empdetail";
    Layout = "~/Views/shared/_Layout1.cshtml";
}

<h2>empdetail</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.emp_username)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.emp_pass)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.emp_email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.emp_username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.emp_pass)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.emp_email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.emp_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.emp_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.emp_id })
        </td>
    </tr>
}

</table>
